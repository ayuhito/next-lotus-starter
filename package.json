{
  "name": "next-lotus-starter",
  "description": "Next.js starter to kickstart my projects.",
  "version": "1.0.0",
  "author": "Ayuhito <declininglotus@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "format:lint": "eslint --fix **/*.{js,jsx,ts,tsx}",
    "format:prettier": "prettier --write **/*.{js,jsx,ts,tsx,css,md,json}",
    "format": "yarn format:lint && yarn format:prettier"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.8",
    "@fontsource/roboto-mono": "^4.5.7",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-accordion": "^0.1.6",
    "@radix-ui/react-icons": "^1.1.1",
    "@stitches/react": "^1.2.8",
    "next": "^12.1.6",
    "next-themes": "^0.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "next-sitemap": "^3.0.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayuhito/next-lotus-starter"
  },
  "bugs": {
    "url": "https://github.com/ayuhito/next-lotus-starter/issues"
  }
}
